---
import BaseLayout from '../layouts/BaseLayout.astro';
import Nav from '../components/Nav.astro';
import Hero from '../components/Hero.astro';
import Footer from '../components/Footer.astro';
import ContactForm from '../components/ContactForm.astro';
import PracticeAreaCard from '../components/PracticeAreaCard.astro';
import FeatureCard from '../components/FeatureCard.astro';
import SchemaOrg from '../components/SchemaOrg.astro';
import { practiceAreas } from '../data/practiceAreas';

const features = [
  {
    title: 'Über 15 Jahre Erfahrung',
    description: 'Seit 2009 vertreten wir erfolgreich Mandanten in Zürich und Umgebung. Unsere Rechtsanwälte kennen die lokalen Gerichte, Behörden und rechtlichen Besonderheiten des Kantons Zürich.',
  },
  {
    title: 'Persönliche Betreuung durch Ihren Anwalt',
    description: 'Bei uns werden Sie nicht zur Nummer. Ihr Fall wird persönlich von einem erfahrenen Rechtsanwalt betreut — vom Erstgespräch bis zum erfolgreichen Abschluss.',
  },
  {
    title: 'Kostenlose Erstberatung',
    description: 'Lernen Sie uns unverbindlich kennen. In einem kostenlosen Erstgespräch analysieren wir Ihre rechtliche Situation und zeigen Ihnen konkrete Handlungsoptionen auf.',
  },
  {
    title: 'Transparente Kosten',
    description: 'Keine versteckten Gebühren. Wir informieren Sie vorab über unser Honorar, arbeiten auf Wunsch mit Pauschalpreisen und bieten bei Bedarf Ratenzahlung an.',
  },
];

const faqs = [
  {
    question: 'Was kostet ein Anwalt in Zürich?',
    answer: 'Der Stundenansatz für einen Rechtsanwalt in Zürich liegt in der Regel zwischen CHF 250 und CHF 500 pro Stunde — abhängig von der Komplexität des Falls und der Erfahrung des Anwalts. In unserer Anwaltskanzlei bieten wir eine kostenlose Erstberatung an, damit Sie ohne finanzielles Risiko erfahren, welche Optionen Sie haben und mit welchen Kosten Sie rechnen können.',
  },
  {
    question: 'Wie finde ich den richtigen Anwalt in Zürich für meinen Fall?',
    answer: 'Achten Sie auf drei Kriterien: Spezialisierung im relevanten Rechtsgebiet, Erfahrung mit vergleichbaren Fällen und eine transparente Kommunikation. Unsere Anwaltskanzlei in Zürich deckt alle wichtigen Rechtsgebiete ab — von Familienrecht über Strafrecht bis hin zu Arbeitsrecht. Im kostenlosen Erstgespräch prüfen wir, ob wir der richtige Partner für Ihr Anliegen sind.',
  },
  {
    question: 'Bieten Sie auch Rechtsberatung ausserhalb von Zürich an?',
    answer: 'Ja. Obwohl unsere Anwaltskanzlei in Zürich ansässig ist, vertreten wir Mandanten in der gesamten Deutschschweiz. Beratungen können auch telefonisch oder per Videokonferenz stattfinden. Für Gerichtstermine in anderen Kantonen übernehmen wir die Vertretung selbstverständlich vor Ort.',
  },
  {
    question: 'Wie läuft die Erstberatung in Ihrer Anwaltskanzlei ab?',
    answer: 'Im ersten Schritt schildern Sie uns Ihr Anliegen — telefonisch, per E-Mail oder über unser Kontaktformular. Innerhalb von 24 Stunden erhalten Sie einen Termin für ein kostenloses Erstgespräch. Darin analysieren wir Ihre Situation, erklären die rechtliche Lage verständlich und zeigen Ihnen die nächsten Schritte auf. Erst danach entscheiden Sie, ob Sie uns mandatieren möchten.',
  },
  {
    question: 'In welchen Rechtsgebieten beraten Sie?',
    answer: 'Unsere Anwaltskanzlei in Zürich berät in allen zentralen Rechtsgebieten des Schweizer Rechts: Familienrecht und Scheidungsrecht, Strafrecht und Strafverteidigung, Arbeitsrecht, Erbrecht, Mietrecht, Vertragsrecht, Baurecht, Immobilienrecht und Migrationsrecht. Für jeden Rechtsbereich steht Ihnen ein spezialisierter Rechtsanwalt zur Verfügung.',
  },
  {
    question: 'Wie schnell kann ich einen Termin bei Ihrem Anwalt bekommen?',
    answer: 'In dringenden Fällen — etwa bei einer Verhaftung, einer fristlosen Kündigung oder einer Eheschutzmassnahme — können wir in der Regel noch am selben Tag reagieren. Für reguläre Beratungstermine erhalten Sie innerhalb von 1 bis 3 Werktagen einen Termin in unserer Kanzlei in Zürich.',
  },
];

const schema = {
  "@type": "LegalService",
  "name": "Anwaltskanzlei Zürich",
  "description": "Anwaltskanzlei in Zürich — erfahrene Rechtsanwälte für Familienrecht, Strafrecht, Arbeitsrecht, Erbrecht und weitere Rechtsgebiete. Kostenlose Erstberatung.",
  "url": "https://anwaltskanzleizuerich.ch",
  "telephone": "[TELEFON]",
  "email": "[EMAIL]",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "[ADRESSE]",
    "addressLocality": "Zürich",
    "postalCode": "[PLZ]",
    "addressCountry": "CH",
  },
  "areaServed": {
    "@type": "City",
    "name": "Zürich",
  },
  "priceRange": "CHF 250-500/Stunde",
  "openingHours": "Mo-Fr 08:00-18:00",
};

const faqSchema = {
  "@type": "FAQPage",
  "mainEntity": faqs.map(faq => ({
    "@type": "Question",
    "name": faq.question,
    "acceptedAnswer": {
      "@type": "Answer",
      "text": faq.answer,
    },
  })),
};
---

<BaseLayout
  title="Anwaltskanzlei Zürich | Rechtsanwalt & Rechtsberatung"
  description="Ihre Anwaltskanzlei in Zürich ★ Erfahrene Rechtsanwälte für Familienrecht, Strafrecht, Arbeitsrecht & Erbrecht. Kostenlose Erstberatung ✓ Jetzt anfragen!"
  keywords="Anwaltskanzlei Zürich, Anwalt Zürich, Rechtsanwalt Zürich, Rechtsberatung Zürich, Anwaltskanzlei"
>
    <SchemaOrg schema={schema} slot="schema" />
    <script type="application/ld+json" set:html={JSON.stringify({ "@context": "https://schema.org", ...faqSchema })} slot="schema" />

    <Nav transparent={true} />

    <!-- ═══════════════ HERO ═══════════════ -->
    <Hero
      title="Anwaltskanzlei Zürich"
      subtitle="Erfahrene Rechtsanwälte für Ihre rechtlichen Anliegen"
      description="Persönliche Beratung. Klare Strategie. Erfolgreiche Vertretung — seit über 15 Jahren."
      ctaText="Kostenlose Erstberatung anfragen"
      ctaHref="#kontakt"
      backgroundImage="https://images.unsplash.com/photo-1508962914676-134849a727f0?w=1920&q=80"
      fullHeight={true}
    >
        <div class="mt-12 flex flex-wrap gap-8 text-sm text-white/70">
            <div class="flex items-center gap-2">
                <div class="w-1.5 h-1.5 rounded-full bg-gold"></div>
                Kostenlose Erstberatung
            </div>
            <div class="flex items-center gap-2">
                <div class="w-1.5 h-1.5 rounded-full bg-gold"></div>
                Transparente Kosten
            </div>
            <div class="flex items-center gap-2">
                <div class="w-1.5 h-1.5 rounded-full bg-gold"></div>
                Termin innert 24 Stunden
            </div>
        </div>
    </Hero>

    <!-- ═══════════════ INTRO ═══════════════ -->
    <section class="py-24 bg-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="lg:grid lg:grid-cols-2 lg:gap-20 items-center">
                <div class="reveal">
                    <p class="section-label mb-3">Über unsere Kanzlei</p>
                    <h2 class="font-serif text-3xl md:text-4xl font-bold text-navy leading-snug mb-6">
                        Ihre Anwaltskanzlei in Zürich — kompetent, persönlich, erfolgreich
                    </h2>
                    <div class="w-12 h-0.5 bg-gold mb-8"></div>
                    <div class="space-y-5 text-gray-600 leading-relaxed">
                        <p>
                            Sie suchen einen <strong class="text-navy">Rechtsanwalt in Zürich</strong>, der Ihr Anliegen versteht und sich konsequent für Ihre Interessen einsetzt? Unsere Anwaltskanzlei verbindet juristische Fachkompetenz mit persönlicher Betreuung — damit Sie in jeder rechtlichen Situation die bestmögliche Lösung erhalten. Wir bieten umfassende Rechtsberatung für Privatpersonen und Unternehmen.
                        </p>
                        <p>
                            Seit über 15 Jahren beraten und vertreten wir Mandanten in Zürich und der Deutschschweiz. Ob Scheidung, Strafverfahren, arbeitsrechtlicher Konflikt oder Erbstreit: Unsere erfahrenen Anwälte analysieren Ihre Situation, entwickeln eine klare Strategie und setzen Ihr Recht durch — vor Gericht, vor Behörden oder am Verhandlungstisch. Auch bei komplexen rechtlichen Herausforderungen finden wir eine Lösung.
                        </p>
                        <p>
                            Was uns von anderen Kanzleien unterscheidet? Wir nehmen uns Zeit für Sie. Jeder Mandant erhält einen festen Ansprechpartner, der den Fall von Anfang bis Ende betreut. Und wir sprechen Klartext: Sie erfahren von uns nicht nur, was rechtlich möglich ist, sondern auch, was sinnvoll ist — ehrlich, direkt und verständlich. Bei allen rechtlichen Fragen stehen wir Ihnen zur Seite.
                        </p>
                    </div>
                </div>
                <div class="mt-12 lg:mt-0 reveal">
                    <div class="relative">
                        <img src="https://images.unsplash.com/photo-1589829545856-d10d557cf95f?w=600&q=80" alt="Rechtsberatung in der Anwaltskanzlei Zürich — persönliches Gespräch mit dem Anwalt" class="rounded w-full object-cover aspect-[4/3]" loading="lazy" width="600" height="450" />
                        <div class="absolute -bottom-6 -left-6 bg-navy-dark text-white p-6 rounded hidden lg:block">
                            <span class="block font-serif text-3xl font-bold text-gold">15+</span>
                            <span class="text-xs text-gray-400 tracking-wide uppercase">Jahre Erfahrung</span>
                        </div>
                    </div>
                    <img src="https://images.unsplash.com/photo-1507003211169-0a1dd7228f2d?w=600&q=80" alt="Erfahrene Rechtsanwälte in Zürich beraten Mandanten" class="rounded w-full object-cover aspect-[16/7] mt-4" loading="lazy" width="600" height="262" />
                </div>
            </div>
        </div>
    </section>

    <!-- ═══════════════ RECHTSGEBIETE ═══════════════ -->
    <section id="rechtsgebiete" class="py-24 bg-warm">
        <div class="max-w-7xl mx-auto px-6">
            <div class="max-w-2xl mb-16 reveal">
                <p class="section-label mb-3">Unsere Expertise</p>
                <h2 class="font-serif text-3xl md:text-4xl font-bold text-navy leading-snug mb-4">
                    Rechtsgebiete unserer Anwaltskanzlei
                </h2>
                <div class="w-12 h-0.5 bg-gold mb-6"></div>
                <p class="text-gray-600 leading-relaxed">
                    Ihr Anwalt in Zürich für alle Rechtsgebiete des Schweizer Rechts
                </p>
            </div>
            <img src="https://images.unsplash.com/photo-1505664194779-8beaceb93744?w=1200&q=80" alt="Gerichtssaal Schweiz — Anwalt vertritt Mandanten vor Gericht in Zürich" class="rounded w-full max-h-72 object-cover mb-12 reveal" loading="lazy" width="1200" height="288" />
            <div class="grid sm:grid-cols-2 lg:grid-cols-3 gap-5 reveal">
                {practiceAreas.map((area) => (
                    <PracticeAreaCard
                        title={area.title}
                        description={area.description}
                        icon={area.icon}
                        href={area.href}
                    />
                ))}
            </div>
        </div>
    </section>

    <!-- ═══════════════ ABLAUF ═══════════════ -->
    <section class="py-24 bg-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="max-w-2xl mb-16 reveal">
                <p class="section-label mb-3">So funktioniert es</p>
                <h2 class="font-serif text-3xl md:text-4xl font-bold text-navy leading-snug mb-4">
                    Rechtsberatung in Zürich — so gehen wir vor
                </h2>
                <div class="w-12 h-0.5 bg-gold mb-6"></div>
                <p class="text-gray-600 leading-relaxed">
                    Von der ersten Kontaktaufnahme bis zur Lösung Ihres Falls — in drei klaren Schritten.
                </p>
            </div>
            <div class="grid md:grid-cols-3 gap-8 reveal">
                <div class="relative">
                    <span class="font-serif text-6xl font-bold text-gold/15 absolute -top-4 -left-2">01</span>
                    <img src="https://images.unsplash.com/photo-1423666639041-f56000c27a9a?w=400&q=80" alt="Telefonische Rechtsberatung — Anwalt in Zürich kontaktieren" class="rounded w-full h-48 object-cover mb-5 relative" loading="lazy" width="400" height="192" />
                    <h3 class="font-serif text-xl font-bold text-navy mb-2">Kontakt aufnehmen</h3>
                    <p class="text-sm text-gray-600 leading-relaxed">
                        Schildern Sie uns Ihr Anliegen — per Telefon, E-Mail oder Kontaktformular. Wir melden uns innerhalb von 24 Stunden mit einem Terminvorschlag.
                    </p>
                </div>
                <div class="relative">
                    <span class="font-serif text-6xl font-bold text-gold/15 absolute -top-4 -left-2">02</span>
                    <img src="https://images.unsplash.com/photo-1521791136064-7986c2920216?w=400&q=80" alt="Kostenlose Erstberatung beim Rechtsanwalt in Zürich" class="rounded w-full h-48 object-cover mb-5 relative" loading="lazy" width="400" height="192" />
                    <h3 class="font-serif text-xl font-bold text-navy mb-2">Kostenlose Erstberatung</h3>
                    <p class="text-sm text-gray-600 leading-relaxed">
                        Im persönlichen Gespräch analysieren wir Ihre Situation, erklären die Rechtslage verständlich und zeigen Ihnen alle Optionen auf — kostenlos und unverbindlich.
                    </p>
                </div>
                <div class="relative">
                    <span class="font-serif text-6xl font-bold text-gold/15 absolute -top-4 -left-2">03</span>
                    <img src="https://images.unsplash.com/photo-1450101499163-c8848c66ca85?w=400&q=80" alt="Strategie und Umsetzung durch erfahrene Anwälte in Zürich" class="rounded w-full h-48 object-cover mb-5 relative" loading="lazy" width="400" height="192" />
                    <h3 class="font-serif text-xl font-bold text-navy mb-2">Strategie und Umsetzung</h3>
                    <p class="text-sm text-gray-600 leading-relaxed">
                        Sie entscheiden — wir handeln. Ihr Rechtsanwalt entwickelt eine massgeschneiderte Strategie und setzt Ihr Recht konsequent durch, ob am Verhandlungstisch oder vor Gericht.
                    </p>
                </div>
            </div>
            <div class="mt-14 reveal">
                <a href="#kontakt" class="inline-flex items-center gap-2 bg-gold text-navy-dark px-8 py-4 rounded text-sm font-semibold hover:bg-gold-light transition">
                    Jetzt kostenlos beraten lassen
                    <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path></svg>
                </a>
            </div>
        </div>
    </section>

    <!-- ═══════════════ WARUM WIR ═══════════════ -->
    <section id="warum" class="py-24 bg-warm">
        <div class="max-w-7xl mx-auto px-6">
            <div class="lg:grid lg:grid-cols-2 lg:gap-20 items-start">
                <div class="reveal">
                    <p class="section-label mb-3">Unsere Vorteile</p>
                    <h2 class="font-serif text-3xl md:text-4xl font-bold text-navy leading-snug mb-4">
                        Warum Mandanten unsere Anwaltskanzlei in Zürich wählen
                    </h2>
                    <div class="w-12 h-0.5 bg-gold mb-8"></div>
                    <img src="https://images.unsplash.com/photo-1497366216548-37526070297c?w=800&q=80" alt="Moderne Anwaltskanzlei in Zürich — professionelle Arbeitsumgebung" class="rounded w-full object-cover aspect-[16/9] mb-8 lg:mb-0" loading="lazy" width="800" height="450" />
                </div>
                <div class="space-y-10 reveal">
                    {features.map((feature, i) => (
                        <FeatureCard title={feature.title} description={feature.description} number={String(i + 1).padStart(2, '0')} />
                    ))}
                </div>
            </div>
        </div>
    </section>

    <!-- ═══════════════ STATISTIKEN ═══════════════ -->
    <section class="py-16 bg-navy-dark text-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="grid grid-cols-2 md:grid-cols-4 gap-8 text-center reveal">
                <div>
                    <span class="font-serif text-4xl md:text-5xl font-bold text-gold">15+</span>
                    <p class="text-xs text-gray-400 mt-2 tracking-wide uppercase">Jahre Erfahrung</p>
                </div>
                <div>
                    <span class="font-serif text-4xl md:text-5xl font-bold text-gold">9</span>
                    <p class="text-xs text-gray-400 mt-2 tracking-wide uppercase">Rechtsgebiete</p>
                </div>
                <div>
                    <span class="font-serif text-4xl md:text-5xl font-bold text-gold">98%</span>
                    <p class="text-xs text-gray-400 mt-2 tracking-wide uppercase">Zufriedenheit</p>
                </div>
                <div>
                    <span class="font-serif text-4xl md:text-5xl font-bold text-gold">24h</span>
                    <p class="text-xs text-gray-400 mt-2 tracking-wide uppercase">Erstreaktion</p>
                </div>
            </div>
        </div>
    </section>

    <!-- ═══════════════ FAQ ═══════════════ -->
    <section class="py-24 bg-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="lg:grid lg:grid-cols-[1fr_2fr] lg:gap-16">
                <div class="mb-12 lg:mb-0 reveal">
                    <p class="section-label mb-3">FAQ</p>
                    <h2 class="font-serif text-3xl md:text-4xl font-bold text-navy leading-snug mb-4">
                        Häufige Fragen an unsere Anwaltskanzlei
                    </h2>
                    <div class="w-12 h-0.5 bg-gold mb-6"></div>
                    <img src="https://images.unsplash.com/photo-1589994965851-a8f479c573a9?w=600&q=80" alt="Häufige Fragen zur Rechtsberatung — Anwalt Zürich beantwortet Ihre Anliegen" class="rounded w-full object-cover aspect-[4/3] mt-6 hidden lg:block" loading="lazy" width="600" height="450" />
                </div>
                <div class="space-y-3 reveal">
                    {faqs.map((faq) => (
                        <details class="group border-b border-gray-200">
                            <summary class="flex items-center justify-between cursor-pointer py-5 text-navy font-semibold hover:text-gold transition">
                                <span class="pr-4">{faq.question}</span>
                                <svg class="w-4 h-4 flex-shrink-0 text-gold transition-transform group-open:rotate-45" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"></path>
                                </svg>
                            </summary>
                            <div class="pb-5 text-sm text-gray-600 leading-relaxed max-w-xl">
                                {faq.answer}
                            </div>
                        </details>
                    ))}
                </div>
            </div>
        </div>
    </section>

    <!-- ═══════════════ KONTAKT ═══════════════ -->
    <section id="kontakt" class="py-24 bg-warm">
        <div class="max-w-7xl mx-auto px-6">
            <div class="lg:grid lg:grid-cols-[1fr_1.2fr] lg:gap-16">
                <!-- Left: Info -->
                <div class="mb-12 lg:mb-0 reveal">
                    <p class="section-label mb-3">Kontakt</p>
                    <h2 class="font-serif text-3xl md:text-4xl font-bold text-navy leading-snug mb-4">
                        Kontaktieren Sie unsere Anwaltskanzlei in Zürich
                    </h2>
                    <div class="w-12 h-0.5 bg-gold mb-6"></div>
                    <p class="text-gray-600 leading-relaxed mb-8">
                        Schildern Sie uns Ihr Anliegen — wir melden uns innert 24 Stunden. <strong class="text-navy">Erstberatung kostenlos und unverbindlich.</strong>
                    </p>

                    <div class="space-y-5">
                        <div class="flex items-center gap-4">
                            <div class="w-10 h-10 bg-navy-dark rounded flex items-center justify-center flex-shrink-0">
                                <svg class="w-4 h-4 text-gold" fill="currentColor" viewBox="0 0 20 20"><path d="M2 3a1 1 0 011-1h2.153a1 1 0 01.986.836l.74 4.435a1 1 0 01-.54 1.06l-1.548.773a11.037 11.037 0 006.105 6.105l.774-1.548a1 1 0 011.059-.54l4.435.74a1 1 0 01.836.986V17a1 1 0 01-1 1h-2C7.82 18 2 12.18 2 5V3z"></path></svg>
                            </div>
                            <div>
                                <p class="text-xs text-gray-500 mb-0.5">Telefon</p>
                                <a href="tel:[TELEFON]" class="text-navy font-medium hover:text-gold transition">[TELEFON]</a>
                            </div>
                        </div>
                        <div class="flex items-center gap-4">
                            <div class="w-10 h-10 bg-navy-dark rounded flex items-center justify-center flex-shrink-0">
                                <svg class="w-4 h-4 text-gold" fill="currentColor" viewBox="0 0 20 20"><path d="M2.003 5.884L10 9.882l7.997-3.998A2 2 0 0016 4H4a2 2 0 00-1.997 1.884z"></path><path d="M18 8.118l-8 4-8-4V14a2 2 0 002 2h12a2 2 0 002-2V8.118z"></path></svg>
                            </div>
                            <div>
                                <p class="text-xs text-gray-500 mb-0.5">E-Mail</p>
                                <a href="mailto:[EMAIL]" class="text-navy font-medium hover:text-gold transition">[EMAIL]</a>
                            </div>
                        </div>
                        <div class="flex items-center gap-4">
                            <div class="w-10 h-10 bg-navy-dark rounded flex items-center justify-center flex-shrink-0">
                                <svg class="w-4 h-4 text-gold" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M5.05 4.05a7 7 0 119.9 9.9L10 18.9l-4.95-4.95a7 7 0 010-9.9zM10 11a2 2 0 100-4 2 2 0 000 4z" clip-rule="evenodd"></path></svg>
                            </div>
                            <div>
                                <p class="text-xs text-gray-500 mb-0.5">Adresse</p>
                                <span class="text-navy font-medium">[ADRESSE], [PLZ ORT]</span>
                            </div>
                        </div>
                        <div class="flex items-center gap-4">
                            <div class="w-10 h-10 bg-navy-dark rounded flex items-center justify-center flex-shrink-0">
                                <svg class="w-4 h-4 text-gold" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.828a1 1 0 101.415-1.414L11 9.586V6z" clip-rule="evenodd"></path></svg>
                            </div>
                            <div>
                                <p class="text-xs text-gray-500 mb-0.5">Öffnungszeiten</p>
                                <span class="text-navy font-medium">Montag — Freitag, 08:00 — 18:00</span>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Right: Form -->
                <div class="bg-white rounded-lg border border-gray-200 p-8 md:p-10 reveal">
                    <h3 class="font-serif text-xl font-bold text-navy mb-1">Schreiben Sie uns</h3>
                    <p class="text-sm text-gray-500 mb-6">Wir antworten innerhalb von 24 Stunden.</p>
                    <ContactForm />
                </div>
            </div>
        </div>
    </section>

    <Footer />
</BaseLayout>
